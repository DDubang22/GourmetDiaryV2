ry {\n        const response = await axios.get('/map');\n        const markerInfosFromServer = response.data; // Assuming the response is an array of items\n        console.log(markerInfosFromServer);\n        const markerInfosArray = Array.isArray(markerInfosFromServer) ? markerInfosFromServer : [markerInfosFromServer];\n        markerInfosArray.forEach(markerInfo => {\n          // markerInfo를 사용하여 원하는 작업 수행\n          const newMarkerInfo = {\n            position: new window.kakao.maps.LatLng(markerInfo.latitude, markerInfo.longitude),\n            shop: {\n              name: markerInfo.shopName,\n              address: markerInfo.shopAddress,\n              postalCode: markerInfo.shopPostalCode,\n              jibun: markerInfo.shopJibun,\n              website: markerInfo.shopWebsiteAddress,\n              image: markerInfo.shopImageAddress\n            }\n          };\n\n          // this.markerInfos 배열에 새로운 데이터 추가\n          this.markerInfos = reactive(Array.from(this.markerInfos).concat(newMarkerInfo));\n        });\n        console.log(this.markerInfos);\n      } catch (error) {\n        console.error('Error fetching data', error);\n      }\n    },\n    reloadMap() {\n      // 로드맵을 다시 그리는 로직을 구현하세요.\n      // 예: this.loadMap();\n      console.log(\"MarkerInfos updated. Reload the map.\");\n      this.loadMap(); // 예시로 loadMap 함수를 호출했습니다. 실제로는 로드맵을 다시 그리는 로직을 추가하세요.\n    }\n  },\n\n  components: {\n    Modal\n  }\n};","map":{"version":3,"names":["Modal","axios","reactive","watch","name","data","map","open_modal","shop","overlay","marker","markerInfos","mounted","window","kakao","maps","loadMap","script","document","createElement","onload","load","src","head","appendChild","reloadMap","methods","fetchData","container","getElementById","options","center","LatLng","level","Map","addMarker","push","position","address","postalCode","jibun","website","image","forEach","markerInfo","Marker","content","CustomOverlay","getPosition","event","addListener","getMap","se